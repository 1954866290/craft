<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zkwp.system.dao.UserDao" >

    <select id="getUserState" parameterType="java.lang.String" resultType="map">
        select state,nickname,headurl,introduction,gender
        from sys_user
        where username = #{username}
    </select>

</mapper>