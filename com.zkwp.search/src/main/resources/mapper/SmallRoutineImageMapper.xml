<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zkwp.search.dao.SmallRoutineImageDao" >
<select id="getSwiperDataByImageName" parameterType="String" resultType="com.zkwp.api.bean.UserPublic">
    select * from biz_user_public_info where imagename = #{imageName}
</select>
<select id="getImageName" parameterType="String" resultType="com.zkwp.api.bean.UserPublic">
    select * from biz_user_public_info where username = #{imageName}
</select>
<select id="getTypes" resultType="com.zkwp.api.bean.Types">
    select * from sys_type
</select>
</mapper>